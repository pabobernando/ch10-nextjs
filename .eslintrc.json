{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended",
        "airbnb"
    ],
    "settings": {
        "import/extensions": [
          ".js",
          ".jsx"
        ]
      },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    
    "rules": {
        "parser": "babel-eslint",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "linebreak-style": ["error", "windows"],
        "no-undef": ["error", { "typeof": true }],
        "react/prop-types": 0,
        "no-console":"warn",
        "no-unused-vars":"warn",
        "comma-dangle":0,
        "no-alert": 1,
        "no-plusplus":0,
        "avoidEscape": true,
        "allowTemplateLiterals": true
    }
}
